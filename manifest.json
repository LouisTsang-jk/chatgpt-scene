{
  "manifest_version": 3,
  "name": "ChatGPT Scene",
  "description": "",
  "version": "0.1",
  "action": {
    "default_popup": "/index.html"
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Ctrl+B",
        "mac": "Command+B"
      }
    }
  },
  "content_scripts": [
    {
      "matches": ["https://chat.openai.com/*"],
      "js": ["/content.js"]
    }
  ]
}